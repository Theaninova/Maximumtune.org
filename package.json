{
  "name": "mmtorg",
  "version": "0.0.1",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "dev:external": "vite dev --host 0.0.0.0",
    "run:android": "npx cap run android",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "optimize": "node optimize-images.js",
    "deploy": "node ./build/index.js",
    "preview": "vite preview --host 0.0.0.0 --port 4520"
  },
  "devDependencies": {
    "@capacitor/android": "4.6.1",
    "@capacitor/cli": "4.6.1",
    "@capacitor/core": "4.6.1",
    "@sveltejs/adapter-static": "1.0.0-next.50",
    "@sveltejs/kit": "1.0.0-next.588",
    "@types/three": "0.146.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "eslint-plugin-unicorn": "45.0.2",
    "imagemin": "8.0.1",
    "imagemin-webp": "7.0.0",
    "mdsvex": "0.10.6",
    "prettier": "2.8.1",
    "prettier-plugin-svelte": "2.9.0",
    "sass": "1.56.2",
    "svelte": "3.55.0",
    "svelte-check": "2.10.2",
    "svelte-preprocess": "5.0.0",
    "swiper": "8.4.5",
    "three": "0.147.0",
    "tslib": "2.4.1",
    "typescript": "4.9.4",
    "vite-plugin-pwa": "0.14.0"
  },
  "type": "module"
}
